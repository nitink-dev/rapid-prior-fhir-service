{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"title": "ImagingSeries",
	"$id": "imaging-series.json",
	"type": "object",
	"properties": {
		"uid": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"started": {
			"type": "string",
			"format": "date-time"
		},
		"instances": {
			"type": "array",
			"items": {
				"$ref": "imaging-instance.json"
			}
		}
	},
	"required": [
		"uid",
		"description",
		"started",
		"instances"
	],
  "additionalProperties": false
}
