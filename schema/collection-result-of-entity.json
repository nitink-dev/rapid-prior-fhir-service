{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "collection-result-of-entity.json",
  "title": "CollectionResult",
  "$comment": "Generic collection result schema for entities",
  "type": "object",
  "properties": {
    "entities": {
      "type": "array",
      "items": {
        "type": "any"
      }
    },
    "page": {
      "type": "number"
    },
    "recordsPerPage": {
      "type": "number"
    },
    "totalRecords": {
      "type": "number"
    },
    "totalRecordsFiltered": {
      "type": "number"
    },
    "queryParams": {
      "$ref": "query-params.json"
    }
  },
  "required": [
    "entities",
    "page",
    "recordsPerPage",
    "totalRecords",
    "totalRecordsFiltered",
    "queryParams"
  ],
  "additionalProperties": false
}
