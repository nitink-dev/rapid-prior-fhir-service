{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "ImagingStudy",
  "$id": "imaging-study.json",
  "type": "object",
  "properties": {
    "uid": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "modality": {
      "type": "string"
    },
    "numberOfSeries": {
      "type": "integer"
    },
    "numberOfInstances": {
      "type": "integer"
    },
    "series": {
      "type": "array",
      "items": {
        "$ref": "imaging-series.json"
      }
    }
  },
  "required": [
    "uid",
    "description",
    "modality",
    "numberOfSeries",
    "numberOfInstances",
    "series"
  ],
  "additionalProperties": false
}
